<!DOCTYPE html>
<html>
<head>
  <title>Craigstr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if signed_in? %>
  <div>
    <%= link_to "Sign out", session_path, method: :delete %>
  </div>
<% end %>

<div>
  <%= yield %>
</div>

</body>
</html>
